<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="800.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <TextArea fx:id="inputField" prefWidth="-1.0" wrapText="true" VBox.vgrow="ALWAYS" />
    <Button fx:id="button" mnemonicParsing="false" onAction="#translate" text="Button" VBox.vgrow="ALWAYS" />
    <TextArea fx:id="outputField" prefWidth="-1.0" wrapText="true" VBox.vgrow="ALWAYS" />
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</VBox>
